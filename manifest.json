{
    "manifest_version": 3,
    "name": "Tab Manager",
    "version": "1.0",
    "description": "Manage your open tabs efficiently",
    "permissions": [
      "tabs",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/favicon-16x16.png",
        "32": "images/favicon-32x32.png",
        "512": "images/android-chrome-512x512.png"
      }
    },
    "background": {
    "service_worker": "script/background.js"
  }
}
  